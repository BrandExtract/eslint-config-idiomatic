{
  "name": "eslint-config-idiomatic",
  "version": "1.0.0",
  "description": "ESLint config for the idiomatic js style",
  "keywords": [
    "idiomatic",
    "eslint",
    "eslintconfig",
    "styleguide",
    "style",
    "guide"
  ],
  "main": "index.js",
  "files": [
    ".eslintrc*",
    "LICENSE",
    "*.js"
  ],
  "scripts": {
    "lint": "eslint ./test/*.spec.js ./test/support/",
    "pretest": "npm run lint",
    "test": "mocha -r ./test/support/support"
  },
  "author": "James Pamplin <james@jamespamplin.com> (http://jamespamplin.com)",
  "repository": "https://github.com/jamespamplin/eslint-config-idiomatic.git",
  "license": "MIT",
  "devDependencies": {
    "eslint": "~2.0.0",
    "chai": "~3.2.0",
    "mocha": "~2.2.5"
  }
}
